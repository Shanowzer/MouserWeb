  function dogeIE_NS6() {  
    doc_width = ns6up?window.innerWidth-20 : iecompattest().clientWidth-20;
		doc_height=(window.innerHeight && dogedistance=="windowheight")? window.innerHeight : (ie4up && dogedistance=="windowheight")?  iecompattest().clientHeight : (ie4up && !window.opera && dogedistance=="pageheight")? iecompattest().scrollHeight : iecompattest().offsetHeight;
    for (i = 0; i < no; ++ i) {
      yp[i] += sty[i];
      if (yp[i] > doc_height-50) {
        xp[i] = Math.random()*(doc_width-am[i]-30);
        yp[i] = 0;
        stx[i] = 0.02 + Math.random()/10;
        sty[i] = 0.7 + Math.random();
      }
      dx[i] += stx[i];
      document.getElementById("dot"+i).style.top=yp[i]+"px";
      document.getElementById("dot"+i).style.left=xp[i] + am[i]*Math.sin(dx[i])+"px";  
    }
    dogetimer=setTimeout("dogeIE_NS6()", 10);
  }

	function hidedoge(){
		if (window.dogetimer) clearTimeout(dogetimer)
		for (i=0; i<no; i++) document.getElementById("dot"+i).style.visibility="hidden"
	}
		

if (ie4up||ns6up){
    dogeIE_NS6();
		if (hidedogetime>0)
		setTimeout("hidedoge()", hidedogetime*2000)
		}

